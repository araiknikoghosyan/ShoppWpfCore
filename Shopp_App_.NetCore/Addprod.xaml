<Window x:Class="Shopp_App_.NetCore.Addprod"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shopp_App_.NetCore"
        mc:Ignorable="d"
        Title="Addprod" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textname" HorizontalAlignment="Left" Margin="253,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textPrice" HorizontalAlignment="Left" Margin="253,217,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <TextBox x:Name="textQuantity" HorizontalAlignment="Left" Margin="253,269,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="nameLabel" Content="Անուն" HorizontalAlignment="Left" Margin="164,162,0,0" VerticalAlignment="Top"/>
        <Label x:Name="priceLabel" Content="Գին" HorizontalAlignment="Left" Margin="177,217,0,0" VerticalAlignment="Top"/>
        <Label x:Name="quantitylabel" Content="Քանակ" HorizontalAlignment="Left" Margin="167,269,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Add" Content="Add" HorizontalAlignment="Left" Margin="585,353,0,0" VerticalAlignment="Top" Click="Add_Click"/>

    </Grid>
</Window>
